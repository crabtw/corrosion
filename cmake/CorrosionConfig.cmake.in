@PACKAGE_INIT@

if (Corrosion_FOUND)
    return()
endif()

list(APPEND CMAKE_MODULE_PATH "${PACKAGE_PREFIX_DIR}/@CORROSION_INSTALL_PREFIX@@CMAKE_INSTALL_DATADIR@/cmake")

if (NOT TARGET Corrosion::Builder)
    add_executable(Corrosion::Builder IMPORTED GLOBAL)

    if(IS_ABSOLUTE @CMAKE_INSTALL_LIBEXECDIR@)
        set(_CORROSION_BUILDER_DESTINATION "@CMAKE_INSTALL_LIBEXECDIR@")
    else()
        set(_CORROSION_BUILDER_DESTINATION "${PACKAGE_PREFIX_DIR}/@CORROSION_INSTALL_PREFIX@@CMAKE_INSTALL_LIBEXECDIR@")
    endif()

    set_property(
        TARGET Corrosion::Builder
        PROPERTY IMPORTED_LOCATION "${_CORROSION_BUILDER_DESTINATION}/corrosion-builder")
endif()

include(Corrosion)
